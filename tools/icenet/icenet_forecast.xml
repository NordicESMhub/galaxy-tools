<tool id="icenet_forecast" name="Icenet Forecast" version="0.1.0+galaxy0" profile="21.05">
    <description>for forecasting sea ice concentration with IceNet</description>
        <requirements>
            <requirement type="package" version= "3.10.9">python</requirement>
            <requirement type="package" version="2023.1.0">xarray</requirement>
            <requirement type="package" version="1.24.2">numpy</requirement>
            <requirement type="package" version="1.5.3">pandas</requirement>
            <requirement type="package" version="1.10.0">scipy</requirement>
            <requirement type="package" version="3.4.0">iris</requirement>
            <requirement type="package" version="1.6.0">netcdf4</requirement>
            <requirement type="package" version="2.15.0">imageio</requirement>
            <requirement type="package" version="3.6.3">matplotlib</requirement>
            <requirement type="package" version="4.64.1">tqdm</requirement>
            <requirement type="package" version="0.5.1">cdsapi</requirement>
            <requirement type="package" version="2.10">tensorflow</requirement>
        </requirements>
        <command detect_errors="exit_code"><![CDATA[
            mkdir -p 'data/network_datasets/dataset1/obs/siconca/abs' &&
            mkdir 'data/network_datasets/dataset1/obs/siconca/linear_trend' && 
            mkdir -p 'data/network_datasets/dataset1/obs/ta500/anom' && 
            mkdir -p 'data/network_datasets/dataset1/obs/psl/anom' && 
            mkdir -p 'data/network_datasets/dataset1/obs/rsds/anom' && 
            mkdir -p 'data/network_datasets/dataset1/obs/rsus/anom' &&
            mkdir -p 'data/network_datasets/dataset1/obs/tas/anom' &&  
            mkdir -p 'data/network_datasets/dataset1/obs/tos/anom' &&
            mkdir -p 'data/network_datasets/dataset1/obs/ua10/abs' &&  
            mkdir -p 'data/network_datasets/dataset1/obs/uas/abs' && 
            mkdir -p 'data/network_datasets/dataset1/obs/vas/abs' && 
            mkdir -p 'data/network_datasets/dataset1/obs/zg250/anom' && 
            mkdir -p 'data/network_datasets/dataset1/obs/zg500/anom' && 
            cp -r '$__tool_directory__/masks' 'data/' && 
            cp -r '$__tool_directory__/meta' 'data/network_datasets/dataset1/' &&
            #for $obs in $siconca_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/siconca/abs/${obs.element_identifier}';
            #end for#
            #for $obs in $linear_trend_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/siconca/linear_trend/${obs.element_identifier}';
            #end for#
            #for $obs in $tas_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/tas/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $ta500_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/ta500/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $tos_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/tos/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $rsds_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/rsds/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $rsus_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/rsus/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $psl_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/psl/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $zg500_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/zg500/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $zg250_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/zg250/anom/${obs.element_identifier}';
            #end for#
            #for $obs in $ua10_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/ua10/abs/${obs.element_identifier}';
            #end for#
            #for $obs in $uas_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/uas/abs/${obs.element_identifier}';
            #end for#
            #for $obs in $vas_dataset#
              ln -s '$obs' 'data/network_datasets/dataset1/obs/vas/abs/${obs.element_identifier}';
            #end for#
            python3 '$__tool_directory__/forecast.py' --siconca '$siconca_file' --config '$__tool_directory__/2021_09_03_1300_icenet_config.json' --models "${",".join(map(str, $models))}" --forecast_start '$forecast_start' --forecast_end '$forecast_end' 
        ]]></command>
        <inputs>
            <param name="forecast_start" type="text"  label="Forecast start date" help="date must have the format yyyy-mm-dd" optional="False"/>
            <param name="forecast_end" type="text"  label="Forecast end date" help="date must have the format yyyy-mm-dd" optional="False"/>
            <param name="siconca_file" type="data" label="Monthly averaged OSI-SAF sea ice concentration data " format="netcdf" help="generated with the Preprocess Icenet Data Tool"/>
            <param name="models" type="data_collection" label="pretrained IceNet ensemble members" help="IceNet consists of 25 ensemble members, this tool is tested with 3 models: 36, 42, 53, which can be downloaded from the Polar Data Centre https://data.bas.ac.uk/full-record.php?id=GB/NERC/BAS/PDC/01526"/>
            <param name="siconca_dataset" type="data_collection" label="Monthly averaged OSI-SAF sea ice concentration data" help="preprocessed with the Preprocess Icenet Data Tool"/>
            <param name="linear_trend_dataset" type="data_collection" label="Linear Trend Forecast" help="calculated with the Preprocess Icenet Data Tool"/>
            <param name="tas_dataset" type="data_collection" label="Monthly averaged ERA5 2m temperature data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="ta500_dataset" type="data_collection" label="Monthly averaged ERA5 pressure level 500 temperature data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="tos_dataset" type="data_collection" label="Monthly averaged ERA5 sea surface temperature data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="rsds_dataset" type="data_collection" label="Monthly averaged ERA5 surface solar radiation downwards data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="rsus_dataset" type="data_collection" label="Monthly averaged ERA5 surface net solar radiation data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="psl_dataset" type="data_collection" label="Monthly averaged ERA5 mean sea level pressure data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="zg500_dataset" type="data_collection" label="Monthly averaged pressure level 500 geopotential data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="zg250_dataset" type="data_collection" label="Monthly averaged ERA5 pressure level 250 geopotential data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="ua10_dataset" type="data_collection" label="Monthly averaged ERA5 pressure level 10 u component of wind data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="uas_dataset" type="data_collection" label="Monthly averaged ERA5 10m u component of wind data" help= "generated with the Preprocess Icenet Data Tool"/>
            <param name="vas_dataset" type="data_collection" label="Monthly averaged ERA5 10m v component of wind data" help= "generated with the Preprocess Icenet Data Tool"/>  
        </inputs>
        <outputs>
            <data name="icenet_forecast" format="netcdf" from_work_dir="data/forecasts/icenet/2021_09_03_1300_icenet_demo/IceNet/icenet_forecasts.nc"/>
        </outputs>
